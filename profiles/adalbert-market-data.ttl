# =============================================================================
# Adalbert Market Data Profile
# Version: 0.2
# Date: 2026-02-03
# =============================================================================
# Market data licensing vocabulary for internal governance.
# =============================================================================

@prefix adalbert:    <https://vocabulary.bigbank/adalbert/> .
@prefix adalbert-gov: <https://vocabulary.bigbank/adalbert/governance/> .
@prefix adalbert-md: <https://vocabulary.bigbank/adalbert/market-data/> .
@prefix odrl:    <http://www.w3.org/ns/odrl/2/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

# =============================================================================
# PROFILE DECLARATION
# =============================================================================

<https://vocabulary.bigbank/adalbert/market-data/> a owl:Ontology, odrl:Profile ;
    dcterms:title "Adalbert Market Data Profile"@en ;
    dcterms:description """
        Market data licensing vocabulary for internal data governance.
        
        Scope: Internal data sharing (provider â†’ consumer).
        For external vendor contracts, see W3C Market Data Profile.
    """@en ;
    dcterms:created "2026-02-03"^^xsd:date ;
    owl:versionInfo "0.2" ;
    owl:imports <https://vocabulary.bigbank/adalbert/governance/> .

# =============================================================================
# TIMELINESS OPERANDS
# =============================================================================

adalbert-md:timeliness a adalbert:LeftOperand ;
    rdfs:label "timeliness"@en ;
    skos:definition "Data freshness category."@en ;
    adalbert:contextKey "timeliness" .

adalbert-md:delayMinutes a adalbert:LeftOperand ;
    rdfs:label "delay minutes"@en ;
    skos:definition "Delay in minutes from real-time."@en ;
    adalbert:contextKey "delayMinutes" .

# Timeliness values
adalbert-md:realtime a skos:Concept ; rdfs:label "real-time"@en .
adalbert-md:delayed a skos:Concept ; rdfs:label "delayed"@en .
adalbert-md:endOfDay a skos:Concept ; rdfs:label "end of day"@en .
adalbert-md:historical a skos:Concept ; rdfs:label "historical"@en .
adalbert-md:embargoed a skos:Concept ; rdfs:label "embargoed"@en .

# =============================================================================
# ASSET OPERANDS
# =============================================================================

adalbert-md:assetClass a adalbert:LeftOperand ;
    rdfs:label "asset class"@en ;
    skos:definition "Financial instrument classification."@en ;
    adalbert:contextKey "assetClass" .

adalbert-md:market a adalbert:LeftOperand ;
    rdfs:label "market"@en ;
    skos:definition "Trading venue or market."@en ;
    adalbert:contextKey "market" .

adalbert-md:isBenchmark a adalbert:LeftOperand ;
    rdfs:label "is benchmark"@en ;
    skos:definition "Whether asset is a benchmark/index."@en ;
    adalbert:contextKey "isBenchmark" .

adalbert-md:geographicScope a adalbert:LeftOperand ;
    rdfs:label "geographic scope"@en ;
    skos:definition "Geographic coverage of the data."@en ;
    adalbert:contextKey "geographicScope" .

# =============================================================================
# RECIPIENT OPERANDS
# =============================================================================

adalbert-md:recipient a adalbert:LeftOperand ;
    rdfs:label "recipient"@en ;
    skos:definition "Data recipient identity."@en ;
    adalbert:contextKey "recipient" .

adalbert-md:recipientType a adalbert:LeftOperand ;
    rdfs:label "recipient type"@en ;
    skos:definition "Category of data recipient."@en ;
    adalbert:contextKey "recipientType" .

adalbert-md:internal a skos:Concept ; rdfs:label "internal"@en .
adalbert-md:external a skos:Concept ; rdfs:label "external"@en .
adalbert-md:professional a skos:Concept ; rdfs:label "professional"@en .
adalbert-md:nonProfessional a skos:Concept ; rdfs:label "non-professional"@en .

# =============================================================================
# COUNT OPERANDS
# =============================================================================

adalbert-md:userCount a adalbert:LeftOperand ;
    rdfs:label "user count"@en ;
    skos:definition "Number of users accessing data."@en ;
    adalbert:contextKey "userCount" .

adalbert-md:deviceCount a adalbert:LeftOperand ;
    rdfs:label "device count"@en ;
    skos:definition "Number of devices accessing data."@en ;
    adalbert:contextKey "deviceCount" .

adalbert-md:locationCount a adalbert:LeftOperand ;
    rdfs:label "location count"@en ;
    skos:definition "Number of locations accessing data."@en ;
    adalbert:contextKey "locationCount" .

# =============================================================================
# ACTIONS
# =============================================================================

adalbert-md:display a adalbert:Action ;
    rdfs:label "display"@en ;
    skos:definition "Display data to users."@en ;
    adalbert:includedIn adalbert-gov:use .

adalbert-md:nonDisplay a adalbert:Action ;
    rdfs:label "non-display"@en ;
    skos:definition "Use data for non-display purposes."@en ;
    adalbert:includedIn adalbert-gov:use .

adalbert-md:distribute a adalbert:Action ;
    rdfs:label "distribute"@en ;
    skos:definition "Distribute data."@en ;
    adalbert:includedIn adalbert-gov:distribute .

adalbert-md:derive a adalbert:Action ;
    rdfs:label "derive"@en ;
    skos:definition "Create derived data products."@en ;
    adalbert:includedIn adalbert-gov:derive .

adalbert-md:calculateIndex a adalbert:Action ;
    rdfs:label "calculate index"@en ;
    skos:definition "Use for index calculation."@en ;
    adalbert:includedIn adalbert-md:derive .

adalbert-md:algorithmicTrading a adalbert:Action ;
    rdfs:label "algorithmic trading"@en ;
    skos:definition "Use for automated trading."@en ;
    adalbert:includedIn adalbert-md:nonDisplay .

adalbert-md:notify a adalbert:Action ;
    rdfs:label "notify"@en ;
    skos:definition "Send usage notifications."@en ;
    adalbert:includedIn adalbert-gov:notify .

adalbert-md:report a adalbert:Action ;
    rdfs:label "report"@en ;
    skos:definition "Submit usage reports."@en .

adalbert-md:audit a adalbert:Action ;
    rdfs:label "audit"@en ;
    skos:definition "Permit audit of data usage."@en .

adalbert-md:attach a adalbert:Action ;
    rdfs:label "attach"@en ;
    skos:definition "Attach attribution to derived works."@en .

# =============================================================================
# DERIVATION TYPE (for conditions)
# =============================================================================

adalbert-md:derivationType a adalbert:LeftOperand ;
    rdfs:label "derivation type"@en ;
    skos:definition "Type of derivation being performed."@en ;
    adalbert:contextKey "derivationType" .

adalbert-md:commingled a skos:Concept ; rdfs:label "commingled"@en .
adalbert-md:nonSubstitutive a skos:Concept ; rdfs:label "non-substitutive"@en .
adalbert-md:newProduct a skos:Concept ; rdfs:label "new product"@en .

# =============================================================================
# END OF PROFILE
# =============================================================================
